/* Import fonts */
@import url("http://fonts.googleapis.com/css?family=Bree+Serif|Merriweather:400,300,700,900|Droid+Sans:400,700");



/* Base Styles */
* {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box; }


html {
  background-color: #023E54; 
  background-image: url(images/fallback-gradient.png); 
  background-image: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#035124), to(#09c55a));
  background-image: -webkit-linear-gradient(top, #035124, #09c55a); 
  background-image: -moz-linear-gradient(top, #035124, #09c55a);
  background-image: -o-linear-gradient(top, #035124, #09c55a);
  min-height: 100%;
  height: auto;
}

body  {
  font-family: Merriweather, serif;
  font-size: 1.5em;
