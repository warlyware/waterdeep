  .search .buttons {
    width: 100%;
    float: right;
  }
}

@media all and (max-width: 550px) {
  .search .formgroup {
    width: 100%;
  }
}

/* user List Styles */

.packlist {
  width: 100%;
  margin: 0 auto;
  opacity: .95;
  z-index: 10;
  margin-top: 20px;
}

.packlist ul {
  margin: 0;
  padding: 0;
}

.packlist li {
  width: auto;
  float: left;
  padding: 8px;
  margin: 15px;
  border-radius: 10px;
  margin-bottom: 10px;
  background-color: rgba(177, 146, 92, 0.55);
  -webkit-transition: all 0.3s ease-out;
  -moz-transition: all 0.3s ease-out;
  -o-transition: all 0.3s ease-out;
  transition: all 0.3s ease-out;
}

.packlist li:hover {
  background-color: #EEE8D5;
  cursor: default;

}

.packlist li.expanded {
  width: 100%;
  background-color: #D3E0E3;
}

